<?xml version="1.0"?>
<block>
  <name>MIMO extract subcarriers</name>
  <key>lte_mimo_extract_subcarriers</key>
  <category>lte</category>
  <import>import lte</import>
  <make>lte.mimo_extract_subcarriers($N_rb_dl, $fftl, $rxant)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>FFT length</name>
    <key>fftl</key>
    <type>int</type>
  </param>

  <param>
    <name>RX antennas</name>
    <key>rxant</key>
    <type>int</type>
  </param>

  <param>
    <name>resource blocks</name>
    <key>N_rb_dl</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$fftl</vlen>
    <nports>$rxant</nports>
  </sink>


  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>12*$N_rb_dl*$rxant</vlen>
  </source>


</block>
